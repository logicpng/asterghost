	--
	case stage
	{
		when 0
		{
			"\0\s[0]\b2Hello, %(username)."
			"\0\s[0]\b2How can I help you, %(username)?"
			"\0\s[0]\b2Need something?"
		}
		others
		{
			case vegafront
			{
				when 0 //rigel
				{
					"\0\s[0]Oh,\w8 hi!"
					"\0\s[5]Hi,\w8 %(username)!"
					"\0\s[0]Mm?"
					"\0\s[5]Need me for something?"
					"\0\s[0]Yes?"
					"\0\s[5]Yeeees,\w8 %(username)?"
				}
				others //vega
				{
					"\0\s[22]...?"
					"\0\s[20]...Yes?"
					"\0\s[20]\i[1]Need something?"
					"\0\s[20]Hello."
					"\0\s[22]Can I help you?"
					"\0\s[20]\i[1]%(username)."
					"\0\s[20]Mm."
					"\0\s[22]\i[7]..."
