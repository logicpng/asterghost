{
	case vegafront
	{
		when 0;"\0\s[7]Oh?\w8 \0\s[0]What balloon did you have in mind?"
		others;"\0\s[22]Okay."
	}	
	balloonCheck = "OFF"
	--
	"\x"
	--
	PermissionMenu
}

